<%- include('partials/header'); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="mb-3">
    <label for="titleCompose" class="form-label">Title</label>
    <input name="postTitle" type="text" class="form-control" id="titleCompose">
  </div>
  <div class="mb-3">
    <label for="postCompose" class="form-label">Post</label>
    <textarea name="postBody" class="form-control" id="postCompose" rows="6"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary publish-btn">Publish</button>
</form>


<%- include('partials/footer'); -%>
